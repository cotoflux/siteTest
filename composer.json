{
    "name":"cotoflux/siteTest",
    "type": "library",
    "description": "Checks the login in a url and returns the token",
    "keywords": ["cotoflux", "token", "url", "package"],
    "homepage": "https://github.com/cotoflux/siteTest",
    "license": "MIT",

    "authors": [{
        "name": "Nuria Puig",
        "email": "cotoflux@gmail.com",
        "homepage": "https://github.com/cotoflux",
        "role": "Developer"
    }],
    
    "require": {
        "guzzlehttp/guzzle": "^6.5",
        "league/oauth2-client": "^2.4"
        
    },

    "require-dev": {
        "phpunit/phpunit": "8",
        "symfony/var-dumper": "^5.0"
    },

    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "cotoflux\\siteTest\\":"src/"
        }
    },

    "config": {
        "sort-packages": true
    },

    "extra": {
        "laravel": {
            "providers": [
                "cotoflux\\siteTest\\LaravelServiceProvider"
            ]
            
        }, 
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
        
    }

}
